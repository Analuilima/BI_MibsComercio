  <DataSets>
    <DataSet Name="DS_DW_VENDEDORES">
      <Query>
        <DataSourceName>DS_DW</DataSourceName>
        <CommandText>SELECT V.NOME, T.ANO, T.QUARTO, SUM(F.TOTALITEM) AS TOTAL_VENDA
FROM     FATO AS F INNER JOIN
                  DIM_VENDEDOR AS V ON F.IDVENDEDOR = V.IDSK INNER JOIN
                  DIM_TEMPO AS T ON F.IDTEMPO = T.IDSK
GROUP BY V.NOME, T.ANO, T.QUARTO
ORDER BY TOTAL_VENDA DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NOME">
          <DataField>NOME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ANO">
          <DataField>ANO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TOTAL_VENDA">
          <DataField>TOTAL_VENDA</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="QUARTO">
          <DataField>QUARTO</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
