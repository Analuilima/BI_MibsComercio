  <DataSets>
    <DataSet Name="DS_DW_PRODUTOS">
      <Query>
        <DataSourceName>DS_DW</DataSourceName>
        <CommandText>SELECT TOP (1) P.NOME, SUM(F.TOTALITEM) AS TOTAL_VENDA
FROM     FATO AS F INNER JOIN
                  DIM_PRODUTO AS P ON F.IDPRODUTO = P.IDSK
GROUP BY P.NOME
ORDER BY TOTAL_VENDA DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NOME">
          <DataField>NOME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TOTAL_VENDA">
          <DataField>TOTAL_VENDA</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_DW_PRODUTOS_PIORES">
