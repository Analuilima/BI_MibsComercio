  <DataSets>
    <DataSet Name="DS_DW_REGIAO">
      <Query>
        <DataSourceName>DS_DW_REGIAO</DataSourceName>
        <CommandText>SELECT C.REGIAO, SUM(F.TOTALITEM) AS TOTAL_VENDA
FROM     FATO AS F INNER JOIN
                  DIM_CLIENTE AS C ON F.IDCLIENTE = C.IDSK
GROUP BY C.REGIAO
ORDER BY TOTAL_VENDA DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="REGIAO">
          <DataField>REGIAO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TOTAL_VENDA">
          <DataField>TOTAL_VENDA</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
