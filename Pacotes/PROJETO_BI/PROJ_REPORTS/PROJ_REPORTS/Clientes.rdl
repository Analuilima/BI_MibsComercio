  <DataSets>
    <DataSet Name="DS_DW_CLIENTES">
      <Query>
        <DataSourceName>DS_DW</DataSourceName>
        <CommandText>SELECT TOP (200) C.NOME, C.EMAIL, C.CIDADE, C.ESTADO, C.REGIAO, SUM(F.TOTALITEM) AS TOTAL_VENDA
FROM     FATO AS F INNER JOIN
                  DIM_CLIENTE AS C ON F.IDCLIENTE = C.IDSK
GROUP BY C.NOME, C.EMAIL, C.CIDADE, C.ESTADO, C.REGIAO
ORDER BY TOTAL_VENDA DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NOME">
          <DataField>NOME</DataField>
