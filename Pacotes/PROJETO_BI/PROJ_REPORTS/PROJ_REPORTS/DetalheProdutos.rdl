  <DataSets>
    <DataSet Name="SET_PRODUTO">
      <Query>
        <DataSourceName>DS_PRODUTO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FORNECEDOR">
            <Value>=Parameters!FORNECEDOR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT P.NOME AS PRODUTO, C.NOME AS CATEGORIA
FROM     DIM_PRODUTO AS P INNER JOIN
                  FATO AS FA ON P.IDSK = FA.IDPRODUTO INNER JOIN
                  DIM_FORNECEDOR AS F ON F.IDSK = FA.IDFORNECEDOR INNER JOIN
                  CATEGORIA AS C ON C.IDCATEGORIA = P.ID_CATEGORIA
WHERE F.NOME = @FORNECEDOR </CommandText>
      </Query>
      <Fields>
        <Field Name="PRODUTO">
          <DataField>PRODUTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CATEGORIA">
          <DataField>CATEGORIA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
